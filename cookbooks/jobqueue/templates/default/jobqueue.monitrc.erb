check process job_queue
  with pidfile <%= @pid_file %>
  start program = "/bin/sh -c 'cd /data/cocodot/current/ && script/job_queue_server start <%= @env %> <%= @pid_file %>'"
  stop program  = "/bin/sh -c '/bin/kill -15 `cat <%= @pid_file %>`'"

check process xvfb
  with pidfile <%= @xvfb_pid_file %>
  start program = "/bin/sh -c 'cd /data/cocodot/current/ && script/xvfb_server start <%= @env %> <%= @xvfb_pid_file %>'"
  stop program  = "/bin/sh -c '/bin/kill -15 `cat <%= @pid_file %>`'"
